{
    "builds": [
        {
            "src": "computer_club/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "computer_club/wsgi.py"

        }

    ]
}